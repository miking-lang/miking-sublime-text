      scope: constant.numeric.float.normal.mcore
      set: [pattern-pushed-breakrecord-cond]
    - match: '(-)?[0-9]+'
      scope: constant.numeric.integer.mcore
      set: [pattern-pushed-breakrecord-cond]
    - match: '\b(?:({{builtin_ident}})|({{reserved_ident}})|(_)|({{ident}})|({{conident}}))\b'
